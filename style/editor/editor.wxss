html,
body,
div,
span,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
abbr,
address,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
samp,
small,
strong,
sub,
sup,
var,
b,
i,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section,
summary,
time,
mark,
audio,
video {
  margin: 0;
  outline: 0;
  border: 0;
  padding: 0;
  vertical-align: baseline;
  font-size: 100%;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  box-sizing: border-box;
}
body {
  font-family: 'Helvetica Neue', 'Roboto', 'microsoft yahei';
  -webkit-touch-callout: none;
  -webkit-text-size-adjust: none;
  -webkit-user-select: none;
}
html,
body {
  min-height: 100%;
}
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section,
div,
ul,
li,
h1,
h2,
h3,
h4,
h5,
h6 {
  display: block;
}
ul,
li {
  list-style: none;
}
blockquote,
q {
  quotes: none;
}
blockquote::before,
blockquote::after,
q::before,
q::after {
  content: none;
}
a {
  margin: 0;
  background: transparent;
  padding: 0;
  vertical-align: baseline;
  text-decoration: none;
  font-size: 100%;
}
em,
i {
  font-style: normal;
}
h1,
h2,
h3,
h4,
h5 {
  font-weight: normal;
}
input {
  outline: none;
}
view,
image,
text {
  box-sizing: border-box;
}
button {
  background: transparent!important;
}
.G-hide-overflow-text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.G-hide-2rows-overflow-text {
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.G-border-bottom {
  position: relative;
}
.G-border-bottom::after {
  background: #d5d5d5;
  content: '';
  position: absolute;
  height: 1px;
  bottom: 0;
  transform: scaleY(0.5);
  transform-origin: center center;
  pointer-events: none;
  left: 0;
  right: 0;
}
.G-border-top {
  position: relative;
}
.G-border-top::after {
  background: #d5d5d5;
  content: '';
  position: absolute;
  height: 1px;
  top: 0;
  transform: scaleY(0.5);
  transform-origin: center center;
  pointer-events: none;
  left: 0;
  right: 0;
}
.G-border-right {
  position: relative;
}
.G-border-right::after {
  background: #d5d5d5;
  content: '';
  position: absolute;
  width: 1px;
  right: 0;
  transform: scaleX(0.5);
  transform-origin: center center;
  pointer-events: none;
  top: 0;
  bottom: 0;
}
.G-border-left {
  position: relative;
}
.G-border-left::after {
  background: #d5d5d5;
  content: '';
  position: absolute;
  width: 1px;
  left: 0;
  transform: scaleX(0.5);
  transform-origin: center center;
  pointer-events: none;
  top: 0;
  bottom: 0;
}
.G-border {
  position: relative;
}
.G-border::after {
  border-color: #d5d5d5;
  content: '';
  position: absolute;
  width: 200%;
  height: 200%;
  left: 0;
  top: 0;
  border-width: 1px;
  border-style: solid;
  transform: scale(0.5, 0.5);
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border-radius: 0;
}
/*
    #公共的padding/margin/width/height/line-height数值
    #
        l代表left,
        r代表right,
        t代表top,
        b代表bottom,
        a代表all,
        w代表width,
        h代表height
        lh代表line-height
    #
    #480以上的数值请自理
*/
.wx-editor {
  width: 100%;
  position: absolute;
  min-height: 100%;
  background: #f2f2f2;
  overflow-x: hidden;
}
.wx-editor--header {
  width: 100%;
  height: 130rpx;
  position: fixed;
  background: #FFC345;
  z-index: 8;
}
.wx-editor--header .inner {
  width: 100%;
  margin-top: 40rpx;
  height: 90rpx;
  line-height: 90rpx;
  position: relative;
}
.wx-editor--header .inner .back {
  width: 19rpx;
  height: 100%;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100% auto;
  background-position: center;
  position: absolute;
  left: 20rpx;
}
.wx-editor--header .inner .title {
  text-align: center;
  font-size: 34rpx;
  font-weight: 500;
  max-width: 80%;
  margin: 0 auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.wx-editor--contain {
  width: 100%;
  margin-top: 130rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  padding-top: 24rpx;
  padding-bottom: 24rpx;
}
.wx-editor--contain .contain-title {
  background: #fff;
  display: block;
  height: 100rpx;
  line-height: 100rpx;
  border: 1px solid #e5e5e5;
  border-radius: 10rpx;
  padding-left: 30rpx;
  padding-right: 90rpx;
  font-size: 40rpx;
}
.wx-editor--contain .contain-date {
  font-size: 20rpx;
  position: absolute;
  right: 70rpx;
  margin-top: -54rpx;
  z-index: 2;
}
.wx-editor--contain .contain-editor {
  margin-top: 32rpx;
  margin-bottom: 32rpx;
  font-size: 24rpx;
  height: 120rpx;
  width: 100%;
}
.wx-editor--contain .contain-images image {
  width: 100%;
  height: auto;
  border-radius: 8rpx;
  position: relative;
  margin-bottom: 4rpx;
  z-index: 3;
}
.wx-editor--contain .contain-images image.drag {
  opacity: 0.57;
  width: 95%;
  margin-left: 2.5%;
  z-index: 9;
}
.wx-editor--contain .add-image-box {
  width: 100%;
  height: 200px;
  border-radius: 8rpx;
  border: 1px dashed #b2b2b2;
  position: relative;
}
.wx-editor--contain .add-image-box .add-icon {
  display: block;
  width: 160rpx;
  background-image: url(data:image/png;base64,);
  background-size: 60% auto;
  background-repeat: no-repeat;
  background-position: center top;
  color: #b2b2b2;
  font-size: 36rpx;
  text-align: center;
  padding-top: 88rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.wx-editor--contain .publish-btn {
  margin-top: 208rpx;
  font-size: 40rpx;
  height: 106rpx;
  line-height: 106rpx;
  width: 280rpx;
  margin-bottom: 200rpx;
  color: #1a2234;
  font-weight: 600;
  background: #ffc345!important;
  border-radius: 56rpx;
}
.wx-editor--contain .recycle-bar {
  position: fixed;
  height: 100rpx;
  background: #e85452;
  width: 100%;
  bottom: 0;
  z-index: 8;
  left: 0;
  text-align: center;
}
.wx-editor--contain .recycle-bar .icon {
  display: block;
  width: 27rpx;
  height: 33rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: cover;
  margin: 0 auto;
  margin-top: 14rpx;
}
.wx-editor--contain .recycle-bar .txt {
  font-size: 20rpx;
  color: #fdeeed;
  margin-top: 12rpx;
  display: block;
}
