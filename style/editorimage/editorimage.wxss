  content: none;
}
a {
  margin: 0;
  background: transparent;
  padding: 0;
  vertical-align: baseline;
  text-decoration: none;
  font-size: 100%;
}
em,
i {
  font-style: normal;
}
h1,
h2,
h3,
h4,
h5 {
  font-weight: normal;
}
input {
  outline: none;
}
view,
image,
text {
  box-sizing: border-box;
}
button {
  background: transparent!important;
}
.G-hide-overflow-text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.G-hide-2rows-overflow-text {
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.G-border-bottom {
  position: relative;
}
.G-border-bottom::after {
  background: #d5d5d5;
  content: '';
  position: absolute;
  height: 1px;
  bottom: 0;
  transform: scaleY(0.5);
  transform-origin: center center;
  pointer-events: none;
  left: 0;
  right: 0;
}
.G-border-top {
  position: relative;
}
.G-border-top::after {
  background: #d5d5d5;
  content: '';
  position: absolute;
  height: 1px;
  top: 0;
  transform: scaleY(0.5);
  transform-origin: center center;
  pointer-events: none;
  left: 0;
  right: 0;
}
.G-border-right {
  position: relative;
}
.G-border-right::after {
  background: #d5d5d5;
  content: '';
  position: absolute;
  width: 1px;
  right: 0;
  transform: scaleX(0.5);
  transform-origin: center center;
  pointer-events: none;
  top: 0;
  bottom: 0;
}
.G-border-left {
  position: relative;
}
.G-border-left::after {
  background: #d5d5d5;
  content: '';
  position: absolute;
  width: 1px;
  left: 0;
  transform: scaleX(0.5);
  transform-origin: center center;
  pointer-events: none;
  top: 0;
  bottom: 0;
}
.G-border {
  position: relative;
}
.G-border::after {
  border-color: #d5d5d5;
  content: '';
  position: absolute;
  width: 200%;
  height: 200%;
  left: 0;
  top: 0;
  border-width: 1px;
  border-style: solid;
  transform: scale(0.5, 0.5);
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border-radius: 0;
}
