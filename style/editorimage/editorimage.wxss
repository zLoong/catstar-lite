body {
  font-family: 'Helvetica Neue', 'Roboto', 'microsoft yahei';
  -webkit-touch-callout: none;
  -webkit-text-size-adjust: none;
  -webkit-user-select: none;
}
html,
body {
  min-height: 100%;
}
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section,
div,
ul,
li,
h1,
