.wx-editorimage--cut .wx-cut--content .wx-cut-area {
  position: absolute;
  width: 80%;
  height: 80%;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  border: 1px solid #ffc345;
  display: flex;
  flex-wrap: wrap;
}
.wx-editorimage--cut .wx-cut--content .wx-cut-area li {
  width: 33.33333333%;
  height: 33.33333333%;
  border-top: 1px solid #ffc345;
  border-right: 1px solid #ffc345;
  position: relative;
}
.wx-editorimage--cut .wx-cut--content .wx-cut-area li:nth-child(3n) {
  border-right: 0;
}
.wx-editorimage--cut .wx-cut--content .wx-cut-area li:nth-child(1) {
  border-top: 0;
}
.wx-editorimage--cut .wx-cut--content .wx-cut-area li:nth-child(1)::after,
.wx-editorimage--cut .wx-cut--content .wx-cut-area li:nth-child(1)::before {
  position: absolute;
  content: '';
  top: -18rpx;
  left: -18rpx;
  background: #ffc345;
}
.wx-editorimage--cut .wx-cut--content .wx-cut-area li:nth-child(1)::after {
  width: 76rpx;
  height: 20rpx;
}
.wx-editorimage--cut .wx-cut--content .wx-cut-area li:nth-child(1)::before {
  height: 76rpx;
  width: 20rpx;
}
.wx-editorimage--cut .wx-cut--content .wx-cut-area li:nth-child(2) {
  border-top: 0;
}
.wx-editorimage--cut .wx-cut--content .wx-cut-area li:nth-child(3) {
  border-top: 0;
}
.wx-editorimage--cut .wx-cut--content .wx-cut-area li:nth-child(3)::after {
  position: absolute;
  content: '';
  width: 2000%;
  height: 2000%;
  background: rgba(0, 0, 0, 0.6);
  bottom: 100%;
  left: -1000%;
  z-index: -1;
}
.wx-editorimage--cut .wx-cut--content .wx-cut-area li:nth-child(3)::before {
  position: absolute;
  content: '';
  width: 2000%;
